<h1>All The Wikis!</h1>
<p><%= link_to "New Wiki", new_wiki_path, class: 'btn btn-success' %></p>
<div class="row">
  <div class="col-md-8">
    <% @wikis.each do |wiki| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
            <%= link_to wiki.name, wiki_path(wiki) %>
            
          </h4>
        <small>
         created <%= time_ago_in_words(wiki.created_at) %> ago by <%= wiki.user.name %>
       </small>
        </div>
      </div>
    <% end %>
  </div>
  <div class="col-md-4">
    
  </div>
</div>



