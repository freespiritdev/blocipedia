<div class="col-md-4">
  <h1><%= markdown @wiki.name %></h1>
  
    <%= link_to "Edit", edit_wiki_path, class: 'btn btn-success' %>

    
    <%= link_to "Delete Wiki", @wiki, method: :delete, class: 'btn btn-danger', data: { confirm: 'Are you sure you want to delete this wiki?' } %> 

   <%= link_to "Cancel", wikis_path, class: 'btn btn-warning' %>


    <%= link_to "Remove Collaborators", wiki_collaborators_path(@wiki), class: 'btn btn-info' %>

    <%= link_to "Add Collaborators", new_wiki_collaborator_path(@wiki), class: 'btn btn-info' %>
     
</div>

<% @wiki.collaborators.all %>
<table>
  <% Collaborator.all.each do |user| %>
    <tr>
      <td>
        <%= user.name %>
      </td>
      <td>
        <%= user.email%>
      </td>

    </tr>
      <% yield %>
  <% end %> 
</table>
